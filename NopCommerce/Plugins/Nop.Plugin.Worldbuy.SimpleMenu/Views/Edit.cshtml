@using Nop.Services.Stores
@using Nop.Core.Infrastructure
@using Nop.Web.Framework.Events
@using Nop.Services.Events
@using Nop.Plugin.Worldbuy.SimpleMenu.Models
@model WB_SimpleMenuModel

@{
    //page title
    Layout = "~/Administration/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = T("Nop.Plugin.Worldbuy.Edit").Text +" - " + T("Nop.Plugin.Worldbuy.SimpleMenu").Text;
    //active menu item (system name)
    Html.SetActiveMenuItemSystemName("Worldbuy.SimpleMenu");
}
<div class="content-header clearfix">
    <h1 class="pull-left">
        @T("Nop.Plugin.Worldbuy.Edit")
        <small><i class="fa fa-arrow-circle-left"></i>@Html.ActionLink(T("Nop.Plugin.Worldbuy.BackToList").Text, "List")</small>
    </h1>
    
</div>
@Html.Partial("~/Plugins/Worldbuy.SimpleMenu/Views/_CreateOrUpdate.cshtml", Model)

